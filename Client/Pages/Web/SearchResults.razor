@page "/search-results"@rendermode InteractiveServer@inherits BaseComponent<HeadContent>    <meta name="description" content="Search Results" /></HeadContent><SearchComponent FilterSelected=@Filter QuerySelected=@Query></SearchComponent><RadzenRow Gap="5" RowGap="5">    <RadzenColumn Size="12">        @Query        @Filter    </RadzenColumn></RadzenRow><script>    function BlazorSetTitle(title, filter) {        let titleValue = "";        if (title != null)            titleValue += " - " + title;        if (filter != null)            titleValue += " - " + filter;        document.title = "MyVideoResu.ME - Results" + titleValue;    }</script>